    "@ckeditor/ckeditor5-engine": "^13.1.0",
    "@ckeditor/ckeditor5-font": "^11.1.0",
    "@ckeditor/ckeditor5-paragraph": "^11.0.1",
    "@ckeditor/ckeditor5-undo": "^11.0.1",
    "@ckeditor/ckeditor5-widget": "^11.0.1",
    "eslint": "^5.5.0",
