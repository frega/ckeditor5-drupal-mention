    "husky": "^1.3.1",
    "lint-staged": "^7.0.0"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.7.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frega/ckeditor5-drupal-mention.git"
  },
